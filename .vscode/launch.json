{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Gatsby develop",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/gatsby",
      "args": [
        "develop"
      ],
      "runtimeArgs": [
        "--nolazy"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Gatsby build",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/gatsby",
      "args": [
        "build"
      ],
      "runtimeArgs": [
        "--nolazy"
      ],
      "console": "integratedTerminal"
    }
  ]
}
