    "check:format": "eslint . --max-warnings 0",
    "check:watch-types": "tsc --noEmit --watch",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
