{
  "name": "openscript.ch",
  "homepage": "https://github.com/openscript-ch/openscript.ch",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "openscript",
  "workspaces": [
    "plugins/gatsby-plugin-i18n-l10n"
  ],
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@svgr/webpack": "^6.1.2",
    "dotenv": "^14.2.0",
    "embla-carousel-autoplay": "^6.1.1",
    "embla-carousel-react": "^6.1.1",
    "gatsby": "^4.5.4",
    "gatsby-plugin-emotion": "^7.5.0",
    "gatsby-plugin-google-gtag": "^4.5.0",
    "gatsby-plugin-image": "^2.5.2",
    "gatsby-plugin-manifest": "^4.5.2",
    "gatsby-plugin-offline": "^5.5.2",
    "gatsby-plugin-react-helmet": "^5.5.0",
    "gatsby-plugin-robots-txt": "^1.7.0",
    "gatsby-plugin-sharp": "^4.5.2",
    "gatsby-plugin-sitemap": "^5.5.0",
    "gatsby-plugin-svgr": "^3.0.0-beta.0",
