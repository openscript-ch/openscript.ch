  "scripts": {
    "build": "gatsby build",
    "build:sprite": "svg-sprite -S --Ss sprite.svg --stack-dest . --Sx --stack-example-template content/images/sprite.json.mustache --stack-example-dest ../content/images/sprite.json -l verbose -D static content/images/*.svg",
